<!--div class="container-fluid">
    <div class="row">

        <div class="col"-->
            <h1 class="d-flex justify-content-center mt-2 text-muted">{{head}} </h1>
            <p class="load d-flex justify-content-center"> Welcome to {{title}} </p>

            <div class="row">
                <div class="col-lg-8 col-sm-12">

                    <div class="form-group">
                        <a class="btn btn-light" type="button" href="/add-schedule">Add Schedule <span class="fa fa-plus"></span> </a>
                    </div>

                    {{#data}}
                        <div class="card mb-1 {{_id}}" id="{{_id}}">
                            <div class="card-body">
                                <div class="row">
                                    <!--div-- class="col-1"-->
                                    <!--a type="button" class="mt-3"><span class="fa fa-times mt-3"></span> </a-->
                                    <!--/div-->
                                    <div class="col-8">
                                        <h3 class="">{{titles}}</h3>
                                        <p class="">{{notes}}</p>
                                        <p class="text-muted disabled" style="font-size:11px;">{{prettifyDate created}}</p>
                                    </div>
                                    <div class="col-4">
                                        <div class="form-group mt-4 text-center">
                                            {{#if updated}}
                                                <a role="button" class="times disabled text-danger" aria-disabled="true"><span class="fa fa-times fa-2x"></span> </a>
                                            {{else}}
                                                <!--input type="radio" class="form-control mt-3 radio" name="titles" value="{{_id}}" id='{{_id}}' title="upadate"-->
                                                <!--a-- class="btn btn-light text-muted">Comp</a-->
                                                <a onclick="dd();" href="#" class="check" role="button" ><span class="fa fa-check fa-2x"></span> </a>
                                                <p class="text-muted disabled text-center" style="font-size:11px;"> Due : {{prettify end_date}}</p>
                                            {{/if}}
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    {{/data}}

                    {{#unless data}}
                        <div class="card mb-1 mt-3 bg-info">
                            <div class="card-body">
                                <div class="row">
                                    <h4 class="text-white">No Task has been found</h4>
                                </div>
                            </div>
                        </div>
                    {{/unless}}

                </div>
            </div>

        <!--/div>
    </div>

</div-->

