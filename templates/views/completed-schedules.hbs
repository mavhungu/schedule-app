    <div class="row">
        <div class="col-lg-8 col-sm-12 formContainer">
            {{#data}}
                <div class="card mb-1 {{_id}}" id="{{_id}}">
                    <div class="card-body">
                        <div class="row">
                            <div class="col-10">
                                <h3 class="">{{titles}}</h3>
                                <p class=""> {{notes}}</p>
                                <p class="text-muted disabled" style="font-size: 11px;">{{prettifyDate updated}}</p>
                            </div>
                            <div class="col-2">
                                <!--button class="btn btn-primary mt-3 d-flex justify-content-center">Delete</button-->
                                <!--input type="radio" class="form-control" name="" id="" aria-haspopup="true"-->
                                <div class="form-group mt-4">
                                    <input type="radio" class="form-control delete" name="delete" value="{{_id}}" id='{{_id}}' title="Delete">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            {{/data}}

            {{#unless data}}
                <div class="card mb-1 bg-info mt-3">
                    <div class="card-body">
                        <div class="row">
                            <h4 class="text-white">No Completed Task has been found</h4>
                        </div>
                    </div>
                </div>
            {{/unless}}
        </div>
    </div>
