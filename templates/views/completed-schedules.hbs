<div class="row">
    <div class="col formContainer">
        {{#data}}
            <div class="card mb-1 {{_id}}" id="{{_id}}">
                <div class="card-body">
                    <div class="row">
                        <div class="col-10">
                            <h3 class="">{{titles}}</h3>
                            <p class=""> {{notes}}</p>
                            <p class="text-muted disabled" style="font-size: 11px;"><span class="nav-icon fa fa-clock"></span> {{prettifyDate updated}}</p>
                        </div>
                        <div class="col-2">
                            <div class="">
                                <input type="radio" class="form-control delete form-check right mt-4" name="delete" value="{{_id}}" id='{{_id}}' title="Delete">
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        {{/data}}

        {{#unless data}}
            <div class="card mb-1 bg-info mt-3">
                <div class="card-body">
                    <div class="text-center">
                        <h4 class="text-white">No Completed Task has been found</h4>
                    </div>
                </div>
            </div>
        {{/unless}}
    </div>
</div>
    <div class="row">
        <div class="col">
        {{#data}}
            <div class="card">
                <div class="card-footer card-comments">
                    <div class="card-comment">
                        <!-- User image -->
                        <!--img class="img-circle img-sm" src="/img/user3-128x128.jpg" alt="User Image"-->
                        <input type="radio" class=" img-sm delete form-check" name="delete" value="{{_id}}" id='{{_id}}' title="Delete">

                        <div class="comment-text">
                    <span class="username">
                      {{titles}}
                      <span class="text-muted float-right"><span class="nav-icon fa fa-clock"></span> 8:03 PM Today</span>
                    </span><!-- /.username -->
                            {{notes}}
                        </div>
                        <!-- /.comment-text -->
                    </div>
                </div>
            </div>
        {{/data}}
        </div>
    </div>