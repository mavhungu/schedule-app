    <div class="row">
        <div class="col-lg-8 col-sm-12">
            {{#data}}
                <div class="card mb-1 {{_id}}" id="{{_id}}">
                    <div class="card-body">
                        <div class="row">
                            <div class="col-10">
                                <h3 class="">{{titles}}</h3>
                                <p class=""> {{notes}}</p>
                                <p class="text-muted disabled" style="font-size:11px;">{{prettifyDate created}}</p>
                            </div>
                            <div class="col-2">
                                <!--button class="btn btn-primary mt-3 d-flex justify-content-center">Complete</button-->
                                <!--input type="radio" class="form-control" name="" id="" aria-haspopup="true"-->
                                <div class="form-group">
                                    <input type="radio" class="form-control mt-4 update radio" name="titles" value='{{_id}}' id='{{_id}}' title="Updates">
                                    <span class=""></span>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            {{/data}}

            {{#unless data}}
                <div class="card mb-1 mt-3 bg-info">
                    <div class="card-body">
                        <div class="row">
                            <h4 class="text-white">No Padding Task has been found</h4>
                        </div>
                    </div>
                </div>
            {{/unless}}
        </div>
    </div>
